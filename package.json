{
    "name": "env-cli",
    "version": "0.1.0",
    "description": "A CLI for copy and paste your local .env to right projects faster",
    "main": "index.js",
    "type": "module",
    "module": "./src/index.ts",
    "bin": {
        "env-cli": "./dist/index.js"
    },
    "engines": {
        "node": ">=18.17.0"
    },
    "scripts": {
        "build": "bun build.ts",
        "test": "bun test",
