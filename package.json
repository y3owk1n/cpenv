    "keywords": [
        "cli",
        "env",
        "node",
        "bun"
    ],
    "author": "Kyle Wong",
    "license": "ISC",
    "devDependencies": {
        "@biomejs/biome": "1.5.2",
        "@types/inquirer": "^9.0.7",
        "@types/minimist": "^1.2.5",
        "bun-types": "latest"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "commander": "^11.1.0",
        "inquirer": "^9.2.12",
        "minimist": "^1.2.8"
    }
}
